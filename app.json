{
    "expo": {
        "name": "Portfolio App",
        "slug": "PortfolioApp",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "bundleIdentifier": "com.koka.portfolio",
            "buildNumber": "1.0.0",
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/logos/pikachu-working-icon.png",
                "backgroundColor": "#1d212d"
            },
            "package": "com.pavankoka1.portfolio"
        },
        "description": "We don't just build apps, we build trust!",
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/logos/pikachu-working-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#1d212d"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "9d8deaa7-ecb3-40a0-abb1-236dc07452f1"
            }
        },
        "owner": "pavankoka1"
    }
}
